{
    "title": "Hyper Misc - Volume, Tilde, Forwared Delete",
    "rules": [
        {
            "description": "Hyper + [/] -> Volume Down/Up ",
            "manipulators": [
                {
                    "from": {
                        "key_code": "close_bracket",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "left_command",
                                "left_control",
                                "left_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "volume_increment"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "open_bracket",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "left_command",
                                "left_control",
                                "left_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "volume_decrement"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Hyper + Backspace -> Forward Delete",
            "manipulators": [
                {
                    "from": {
                        "key_code": "delete_or_backspace",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "left_command",
                                "left_control",
                                "left_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "delete_forward"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Hyper + Esc + Shift -> Tilde",
            "manipulators": [
                {
                    "from": {
                        "key_code": "escape",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "left_command",
                                "left_control",
                                "left_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "grave_accent_and_tilde"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "tab",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "left_command",
                                "left_control",
                                "left_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "grave_accent_and_tilde",
                            "modifiers": [
                                "shift"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Hyper + ' > backtick",
            "manipulators": [
                {
                    "from": {
                        "key_code": "quote",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "left_command",
                                "left_control",
                                "left_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "grave_accent_and_tilde"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Hyper + U > Page Up",
            "manipulators": [
                {
                    "from": {
                        "key_code": "u",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "left_command",
                                "left_control",
                                "left_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "page_up"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Hyper + N > Page Down",
            "manipulators": [
                {
                    "from": {
                        "key_code": "n",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "left_command",
                                "left_control",
                                "left_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "page_down"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Hyper + E > Escape",
            "manipulators": [
                {
                    "from": {
                        "key_code": "e",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "left_command",
                                "left_control",
                                "left_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "escape"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Hyper + U > Page Up",
            "manipulators": [
                {
                    "from": {
                        "key_code": "u",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "left_command",
                                "left_control",
                                "left_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "page_up"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Hyper + N > Page Down",
            "manipulators": [
                {
                    "from": {
                        "key_code": "n",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "left_command",
                                "left_control",
                                "left_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "page_down"
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}